{
    "name": "ikechukwukalu/magicmake",
    "description": "A scaffolding package for an opinionated Laravel coding style.",
    "type": "library",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "Ikechukwukalu\\Magicmake\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ikechukwukalu\\Magicmake\\Tests\\": "Tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "authors": [
        {
            "name": "Ikechukwu Kalu",
            "email": "ea.ikechukwukalu@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "beyondcode/laravel-websockets": "^1.14",
        "calebporzio/sushi": "^2.4",
        "doctrine/dbal": "^3.5",
        "hisorange/browser-detect": "^4.5",
        "ikechukwukalu/clamavfileupload": "^2.0",
        "ikechukwukalu/makeservice": "^1.0",
        "ikechukwukalu/requirepin": "^1.0",
        "illuminate/console": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "kreait/laravel-firebase": "^5.6",
        "knuckleswtf/scribe": "^4.29",
        "laravel/socialite": "^5.5",
        "laravel/ui": "^4.4",
        "laragear/two-factor": "^1.2",
        "php": ">=7.3",
        "predis/predis": "^2.1",
        "pusher/pusher-php-server": "^7.2",
        "sentry/sentry-laravel": "^4.1",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-permission": "^6.3",
        "stevebauman/location": "^6.6",
        "symfony/console": "^5.4|^6.0",
        "symfony/finder": "^6.0",
        "mobiledetect/mobiledetectlib": "2.8.32",
        "react/http": "1.10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0",
        "php-parallel-lint/php-parallel-lint": "dev-develop"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ikechukwukalu\\Magicmake\\MagicmakeServiceProvider"
            ]
        }
    }
}
